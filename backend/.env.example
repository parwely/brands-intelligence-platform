# .env.example - Template for environment variables
# Copy this to .env and fill in your actual values

# Application Settings
DEBUG=true
SECRET_KEY=your_secret_key_here

# Database URLs
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/brand_intelligence
REDIS_URL=redis://localhost:6379/0
ELASTICSEARCH_URL=http://localhost:9200
CLICKHOUSE_URL=http://localhost:8123

# Application Info
APP_NAME="Brand Intelligence Platform"
VERSION="1.0.0"

# API Keys (Get your own from respective services)
NEWS_API_KEY=your_news_api_key_here
GOOGLENEWS_QUERY=https://news.google.com/rss/search?q=YOUR_QUERY

# ML Model Paths
SENTIMENT_MODEL_PATH=./ml/models/sentiment_model
CRISIS_MODEL_PATH=./ml/models/crisis_model